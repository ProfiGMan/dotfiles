#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
alias grep='grep --color=auto'
# alias v=nvim
alias f=fuck
PS1='[\u@\h \W]\$ '

eval "$(starship init bash)"

eval "$(thefuck --alias)"

# Created by `pipx` on 2024-01-02 13:26:08
export PATH="$PATH:~/.local/bin"
